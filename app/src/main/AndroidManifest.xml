<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.graviton.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp">
            <intent-filter>
                <action android:name="com.example.graviton.myapplication.SignUp" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ExaminerRegistration">
            <intent-filter>
                <action android:name="com.example.graviton.myapplication.ExaminerRegistration" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Questions" />
        <activity android:name=".AddQuestion" />
        <activity android:name=".TeacherOption" />
        <activity android:name=".SubjectForTeachers" />
        <activity android:name=".ExamDetailSet" />
        <activity android:name=".StudentOption" />
        <activity android:name=".AvailableExamList" />
        <activity android:name=".SubjectChoiceStudent"></activity>
    </application>

</manifest>